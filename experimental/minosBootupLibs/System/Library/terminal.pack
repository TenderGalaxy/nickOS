

globalThis.parseCommand = function(x){
  x = x.split(' ')
  let f = x.shift()
  return eval(`${f}(...[x])`)
}

globalThis.cd = function(x){
  terminalPath = x
  return terminalPath
}
globalThis.home = function(){
  terminalPath = ""
  return
}
