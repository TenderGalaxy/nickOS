function decreaseClick(){
	isClicking = registerClick
	registerClick = false
	if(isClicking){
		while(clickFunctions.length > 0{
			requestExecFunction(clickFunctions.shift(), '')
		}
	}
	requestExecFunction('decreaseClick()', 'tempOutput')
}
function getPlayerLook(){
	let z = api.getPlayerFacingInfo(id).dir
	if(inBounds(z[0], -0.5, 0.5) && inBounds(z[1],-0,5,0,5)){
		cursorPos = [(z[0]*2 + 1) * config.displayx, (z[1]*2 + 1) * config.displayy]
	} else {
		cursorPos =  [0,0]
	}
	requestExecFunction('getPlayerLook()','')
}
function onPlayerClick(id){
  if(id == user){
    registerClick = true
  }
}
decreaseClick()
getPlayerLook()
clickFunctions = []
